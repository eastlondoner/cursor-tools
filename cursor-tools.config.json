{
  "perplexity": {
    "model": "sonar-pro",
    "maxTokens": 8000
  },
  "gemini": {
    "model": "gemini-2.0-pro-exp",
    "maxTokens": 10000
  },
  "openrouter": {
    "model": "anthropic/claude-3-7-sonnet",
    "maxTokens": 10000
  },
  "plan": {
    "fileProvider": "openrouter",
    "thinkingProvider": "openrouter",
    "fileMaxTokens": 8192,
    "thinkingMaxTokens": 8192
  },
  "repo": {
    "provider": "openrouter",
    "maxTokens": 10000
  },
  "doc": {
    "maxRepoSizeMB": 100,
    "provider": "openrouter",
    "maxTokens": 10000
  },
  "browser": {
    "defaultViewport": "1280x720",
    "timeout": 30000,
    "stagehand": {
      "env": "LOCAL",
      "headless": true,
      "verbose": 1,
      "debugDom": false,
      "enableCaching": false,
      "model": "claude-3-7-sonnet-latest",
      "provider": "anthropic",
      "timeout": 30000
    }
  },
  "tokenCount": {
    "encoding": "o200k_base"
  },
  "openai": {
    "maxTokens": 8000
  },
  "anthropic": {
    "maxTokens": 8000
  }
}